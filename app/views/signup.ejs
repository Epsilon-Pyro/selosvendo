<!DOCTYPE html>
<html>
<head>
	<!-- Usar la propiedad 'title' para renderizar un título de página  -->
	<title><%= title %></title>
	<link href="\lib\bootstrap\css\bootstrap.css" rel="stylesheet">
	<link href="\lib\bootstrap\css\estilos.css" rel="stylesheet">
</head>
<body>
  <!-- Mostrar mensajes flash  -->
  <% for(var i in messages) { %>
    <div class="flash"><%= messages[i] %></div>
  <% } %>
  <!-- Renderizar el formulario signup  -->
  
  <form action="/signup" method="post">
  
 <div style="margin:10px;" class="row">
 <div class="col-xs-12	col-sm-6 col-md-6 col-lg-3">
    <div class="form-group">
      <label>Nombre:</label>
      <input type="text" name="firstName" class="form-control"/>
    </div>
    <div class="form-group">
      <label>Apellido:</label>
      <input type="text" name="lastName" class="form-control"/>
    </div>
    <div class="form-group">
      <label>Email:</label>
      <input type="text" name="email" class="form-control"/>
    </div>
    <div class="form-group">
      <label>Nombre Usuario:</label>
      <input type="text" name="username" class="form-control"/>
    </div>
    <div class="form-group">
      <label>Contraseña:</label>
      <input type="password" name="password" class="form-control"/>
    </div>
    <div class="form-group">
      <input type="submit" value="Crear Cuenta" class="btn btn-default"/>
	  <a href="/#!/gestor" class="btn btn-primary"> volver sin guardar</a>
    </div>
	</div>
	</div>
	
  </form>
  

    
	
</body>
</html>